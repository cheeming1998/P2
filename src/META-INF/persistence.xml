<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="FioreFlowerShopPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>entity.Custorder</class>
    <class>entity.Staff</class>
    <class>entity.Product</class>
    <class>entity.Producttype</class>
    <class>entity.Consumer</class>
    <class>entity.Customer</class>
    <class>entity.Orderproduct</class>
    <class>entity.Promotion</class>
    <class>entity.Corporate</class>
    <class>entity.Payment</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:derby://localhost:1527/FioreFlower"/>
      <property name="javax.persistence.jdbc.user" value="abcde"/>
      <property name="javax.persistence.jdbc.driver" value="org.apache.derby.jdbc.ClientDriver"/>
      <property name="javax.persistence.jdbc.password" value="abcde"/>
    </properties>
  </persistence-unit>
</persistence>
